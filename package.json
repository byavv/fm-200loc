{
  "name": "200loc-gate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:build && ./node_modules/.bin/gulp build --env production",
    "build:dev": "npm run clean:build && ./node_modules/.bin/gulp build --env development",
    "start": "cross-env node . --log info --env production",
    "test": "cross-env NODE_ENV=test DEBUG=test istanbul cover node_modules/mocha/bin/_mocha -- -t 5000 -R spec gateway/test/*.spec.js plugins/**/test/*.spec.js",
    "test-cov": "cross-env NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec gateway/test/*.spec.js && cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "dev": "cross-env DEBUG=gateway,plugins:* node . --log debug --env development",
    "pm2": "cross-env pm2 start . --name=200loc -i max --no-daemon --log info --env production",
    "serve": "cross-env ./node_modules/.bin/gulp --env development",
    "clean": "npm run clean:coverage && npm run clean:build",
    "clean:coverage": "rm -rf ./coverage",
    "clean:build": "rm -rf ./explorer/build/*",
    "clean:hard": "npm run clean:build && npm run clean:coverage && rm -rf node_modules",
    "docker:build": "docker build -t aksenchyk/fm200-loc:build ."
  },
  "engines": {
    "node": ">= 6"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
